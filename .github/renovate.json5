{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  'extends': [
    'github>camptocamp/gs-renovate-config-preset:base.json5#1.1.2',
    'github>camptocamp/gs-renovate-config-preset:group.json5#1.1.2',
    'github>camptocamp/gs-renovate-config-preset:stabilization-branches.json5#1.1.2',
    'github>camptocamp/gs-renovate-config-preset:ci.json5#1.1.2',
    'github>camptocamp/gs-renovate-config-preset:preset.json5#1.1.2',
    'github>camptocamp/gs-renovate-config-preset:pre-commit.json5#1.1.2',
    'github>camptocamp/gs-renovate-config-preset:own.json5#1.1.2',
    'github>camptocamp/gs-renovate-config-preset:json-schema.json5#1.1.2',
    'github>camptocamp/gs-renovate-config-preset:shellcheck.json5#1.1.2',
  ],
  separateMultipleMajor: true,
  baseBranches: ['master', '2.7', '2.8', '2.9'],
  customManagers: [
    {
      fileMatch: ['^ci/applications\\-versions\\.yaml$'],
      matchStrings: ['(?<depName>[^\\s]+): (?<currentValue>[^\\s]+) # (?<datasource>[^\\s]+)'],
      customType: 'regex',
    },
    {
      fileMatch: ['^.github/workflows/main\\.yaml$'],
      matchStrings: ["node-version: '(?<currentValue>[^\\s]+).x'"],
      depNameTemplate: 'node',
      datasourceTemplate: 'node',
      customType: 'regex',
    },
  ],
  packageRules: [
    /** Requires dashboard approval for major updates */
    {
      matchUpdateTypes: ['major'],
      dependencyDashboardApproval: true,
    },
    /** Accept only the patch on stabilization branches */
    {
      matchBaseBranches: ['/^[0-9]+\\.[0-9]+$/'],
      matchUpdateTypes: ['major', 'minor', 'pin', 'digest', 'lockFileMaintenance', 'rollback', 'bump'],
      enabled: false,
    },
    /** Group Webpack dependencies */
    {
      matchPackageNames: [
        'webpack',
        '/^webpack-.*/',
        '/.*-webpack-plugin/',
        '/.*-webpack$/',
        '/.*-loader$/',
        'loader-utils',
      ],
      groupName: 'webpack',
      automerge: true,
      separateMultipleMajor: false,
    },
    /** Group TypeScript dependencies */
    {
      matchPackageNames: ['typedoc', 'typescript'],
      groupName: 'typescript',
      automerge: true,
      separateMultipleMajor: true,
    },
    /** Packages published very recently are not pushed to stabilization branches for security reasons */
    {
      matchBaseBranches: ['/^[0-9]+\\.[0-9]+$/'],
      minimumReleaseAge: '7 days',
    },
    /** Disable cheerio update on stabilization branches, patch release contains braking changes */
    {
      matchBaseBranches: ['/^[0-9]+\\.[0-9]+$/'],
      matchPackageNames: ['cheerio'],
      enabled: false,
    },
    /** Group Python dependencies */
    {
      matchPackageNames: ['actions/python-versions'],
      groupName: 'python',
      automerge: true,
    },
  ],
}
